{
  "name": "openapi",
  "version": "1.0.0",
  "description": "An OpenAPI specification for the MagicBell API",
  "repository": "git@github.com:magicbell-io/openapi.git",
  "author": "Stephan Meijer <stephan.meijer@gmail.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "validate": "tsx scripts/validate-openapi.ts",
    "postman:generate": "tsx scripts/generate-postman.ts",
    "smoke-test": "tsx scripts/smoke-test.ts",
    "build:smoke-test": "ncc build scripts/smoke-test.ts -o .github/actions/smoke-test/dist",
    "prepare": "husky install",
    "lint": "eslint --fix ."
  },
  "lint-staged": {
    "*.{md,json,yml,yaml}": [
      "prettier -w"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vercel/ncc": "^0.34.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "ast-types": "^0.14.2",
    "axios": "^0.27.2",
    "chai": "^4.3.6",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.0",
    "json5": "^2.2.1",
    "jsonpath": "^1.1.1",
    "lint-staged": "^13.0.3",
    "listr2": "^5.0.5",
    "openapi-to-postmanv2": "^4.2.0",
    "openapi-types": "^12.0.2",
    "prettier": "^2.7.1",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3",
    "zx": "^7.0.8"
  }
}
